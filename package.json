{
  "name": "nodejs-ts-starter",
  "version": "1.0.0",
  "description": "NodeJs typescript simple starter",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "bw": "tsc -w",
    "build-watch": "npm run bw",
    "clean": "rimraf dist",
    "dev": "nodemon --watch src --ext ts,json --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "keywords": [
    "Nodejs",
    "Typescript",
    "VSCode debug",
    "nodeman",
    "ts-node",
    "jest"
  ],
  "author": "Vincent Mao",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^30.0.0",
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.5.2",
    "@types/xml2js": "^0.4.14",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  },
  "dependencies": {
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.2",
    "minimist": "^1.2.8",
    "moment": "^2.30.1",
    "uuid": "^13.0.0",
    "xml2js": "^0.6.2"
  }
}
